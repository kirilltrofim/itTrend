@model itTrend.Models.Group

@{
    ViewData["Title"] = "Подробно";
}

<h1>Подробно</h1>

<div>
    <h4>Группа</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Номер
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Number)
        </dd>
        <dt class="col-sm-2">
            Год (начала)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.StartYear)
        </dd>
        <dt class = "col-sm-2">
            Специальность
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.specialization)
        </dd>
        <dt class = "col-sm-2">
            Преподаватель
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Educator)
        </dd>
        <dt class = "col-sm-2">
            Курс
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Course)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ID">Изменить</a> |
    <a asp-action="Index">Вернуться назад</a>
</div>
